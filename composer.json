{
    "name": "laraxot/module_user_fila3",
    "description": "user module for user management",
    "keywords": [
        "laraxot",
        "laravel",
        "filament",
        "module_user",
        "Laravel", 
        "User module",
        "Users",
        "Users management"
    ],
    "homepage": "https://github.com/laraxot/module_user_fila3",
    "license": "MIT",
    "authors": [{
        "name": "Marco Sottana",
        "email": "marco.sottana@gmail.com"
    }],
    "extra": {
        "laravel": {
            "providers": [
                "Livewire\\LivewireServiceProvider",
                "Modules\\User\\Providers\\UserServiceProvider"
            ],
            "aliases": {

            }
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\User\\": ""
        }
    },
    "require": {
        "laravel/passport": "*",
        "laraxot/module_tenant_fila3": "*",
        "laraxot/module_xot_fila3": "dev-master",
        "spatie/laravel-personal-data-export": "*",
        "thecodingmachine/safe": "*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "*",
        "driftingly/rector-laravel": "*",
        "enlightn/enlightn": "*",
        "fakerphp/faker": "*",
        "friendsofphp/php-cs-fixer": "*",
        "laravel/pint": "*",
        "nunomaduro/collision": "*",
        "nunomaduro/larastan": "*",
        "nunomaduro/phpinsights": "*",
        "orchestra/testbench": "*",
        "pestphp/pest": "*",
        "pestphp/pest-plugin-arch": "*",
        "pestphp/pest-plugin-laravel": "*",
        "phpstan/phpstan-deprecation-rules": "*",
        "phpstan/phpstan-phpunit": "*",
        "psalm/plugin-laravel": "*",
        "rector/phpstan-rules": "*",
        "rector/rector": "*",
        "spatie/laravel-ray": "*",
        "symplify/phpstan-rules": "*",
        "thecodingmachine/phpstan-safe-rule": "*",
        "tightenco/duster": "*",
        "vimeo/psalm": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../module_xot_fila3"
        },
        {
            "type": "path",
            "url": "../module_tenant_fila3"
        },
        {
            "type": "path",
            "url": "../module_ui_fila3"
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/pest --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
